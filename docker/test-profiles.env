# Test Profile Environment Variables

# LOCAL PROFILE - For local development testing
LOCAL_MAX_RETRIES=10
LOCAL_TIMEOUT_SECONDS=60
LOCAL_API_ENDPOINT=http://localhost:8080
LOCAL_ENABLE_COVERAGE=true
LOCAL_VERBOSE_LOGGING=true

# REMOTE/CI PROFILE - For GitHub Actions and CI environments
REMOTE_MAX_RETRIES=2
REMOTE_TIMEOUT_SECONDS=5
REMOTE_API_ENDPOINT=https://api.example.com
REMOTE_ENABLE_COVERAGE=false
REMOTE_VERBOSE_LOGGING=false

# Common settings
PYTHON_VERSION=3.11
UV_SYSTEM_PYTHON=1
PYTHONDONTWRITEBYTECODE=1
PYTHONUNBUFFERED=1
