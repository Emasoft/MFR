# Example environment file for local act testing
# Copy this file to .env.local and add your actual values

# GitHub token for act (optional, only if needed for private repos)
# GITHUB_TOKEN=<your-github-token-here>

# Docker settings (optional)
# DOCKER_HOST=unix:///var/run/docker.sock

# Python settings
UV_SYSTEM_PYTHON=true
UV_CACHE_DIR=/tmp/.uv-cache

# Skip certain hooks during local testing (optional)
# SKIP=pip-compile,actionlint
